		echo $cellulesPlateau[1][3];

	for ($row = 1; $row <= 5; $row++) 
	{
  		echo "<p><b>Ligne numero $row</b></p>";
		echo "<ul>";
	    for ($col = 1; $col <= 1; $col++) 
		{
		    echo "<li>".$cellulesPlateau[$row][$col]."</li>";
  		}
  		echo "</ul>";
	}

	
